{
  "name": "odoo-xmlrpc-client",
  "version": "1.0.0",
  "description": "A nice greeter",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --config jestconfig.json",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dennybiasiolli/odoo-xmlrpc-client.git"
  },
  "keywords": [
    "odoo",
    "xmlrpc",
    "client"
  ],
  "author": "Denny Biasiolli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dennybiasiolli/odoo-xmlrpc-client/issues"
  },
  "homepage": "https://github.com/dennybiasiolli/odoo-xmlrpc-client#readme",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/xmlrpc": "^1.3.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "url": "^0.11.0",
    "xmlrpc": "^1.3.2"
  }
}
